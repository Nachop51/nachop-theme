{
  "name": "nachop-theme",
  "displayName": "Nachop theme",
  "description": "Mix of the best VSCode themes",
  "version": "0.4.0",
  "publisher": "nachop51",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nachop51/nachop-theme.git"
  },
  "bugs": {
    "url": "https://github.com/Nachop51/nachop-theme/issues",
    "email": "nachoperalta0@gmail.com"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark",
    "light",
    "color",
    "syntax",
    "highlighting",
    "nachop"
  ],
  "homepage": "https://github.com/Nachop51/nachop-theme",
  "contributes": {
    "themes": [
      {
        "label": "Nachop old",
        "uiTheme": "vs-dark",
        "path": "./themes/nachop-old.json"
      },
      {
        "label": "Nachop old bordered",
        "uiTheme": "vs-dark",
        "path": "./themes/nachop-old-bordered.json"
      },
      {
        "label": "Nachop dark",
        "uiTheme": "vs-dark",
        "path": "./themes/nachop-dark.json"
      },
      {
        "label": "Nachop dark bordered",
        "uiTheme": "vs-dark",
        "path": "./themes/nachop-dark-bordered.json"
      },
      {
        "label": "Nachop darker",
        "uiTheme": "vs-dark",
        "path": "./themes/nachop-darker.json"
      },
      {
        "label": "Nachop darker bordered",
        "uiTheme": "vs-dark",
        "path": "./themes/nachop-darker-bordered.json"
      }
    ]
  },
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .json",
    "prepublish": "npm run lint",
    "build": "bun ./src/build.ts",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.4",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/vsce": "^2.26.1",
    "chroma-js": "^2.4.2",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  }
}

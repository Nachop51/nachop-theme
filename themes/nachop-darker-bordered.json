	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#546e7a"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.parameter.function-call",
				"variable.language"
			],
			"settings": {
				"foreground": "#ee6584"
			}
		},
		{
			"name": "Const variables",
			"scope": [
				"variable.other.constant",
				"constant.other.color"
			],
			"settings": {
				"foreground": "#f5dd87"
			}
		},
		{
			"name": "Language defined stuff (functions, methods, etc)",
